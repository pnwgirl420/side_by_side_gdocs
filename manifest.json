{
  "manifest_version": 2,
  "name": "Side-by-side view for Google Docs",
  "description": "Bring a side-by-side view toggler to Google Docs.",
  "version": "0.1.1",
  "content_scripts": [{
    "run_at": "document_end",
    "matches": [
      "https://docs.google.com/*",
      "https://drive.google.com/*"
    ],
    "js": [
      "side_by_side_gdocs.js"
    ]
  }]
}
